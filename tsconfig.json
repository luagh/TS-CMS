{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["src/**/*", "src/**/*.vue", "env.d.ts"],
  "compilerOptions": {
    "types": ["@vue/runtime-core"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noImplicitAny": false //默认any
  },

  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
